<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.myGame">

    <application
        android:allowBackup="true"
        android:extractNativeLibs="true"
        android:icon="@drawable/ic_car_svgrepo_com"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:manageSpaceActivity=".JMESurfaceViewExampleActivity.JmESurfaceViewExample"
        android:roundIcon="@drawable/ic_car_svgrepo_com"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".JmEGamePadExample.LanLogic" />
        <activity android:name=".JMESurfaceViewExampleActivity.TestException" />
        <activity
            android:name=".JMESurfaceViewExampleActivity.JmESurfaceViewExample"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JmEGamePadExample.BluetoothLogic"
            android:screenOrientation="landscape" />
    </application>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-permission android:name="android.permission.BLUETOOTH" />

</manifest>